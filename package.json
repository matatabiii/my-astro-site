{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn astro dev\" \"yarn dev:svgSprite\"",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "dev:svgSprite": "chokidar './public/assets/svg/icons/*.svg' -c 'yarn svg-sprite' --initial",
    "svg-sprite": "svg-sprite -symbol --symbol-dest ./public/assets/svg --shape-id-separator '' --shape-id-generator 'i-%s' --symbol-sprite sprite.svg --svg-xmldecl false --shape-transform-svgo svgo.config.js ./public/assets/svg/icons/*.svg"
  },
  "dependencies": {
    "@astrojs/image": "^0.9.2",
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-intersection": "^0.2.0",
    "sass-easing": "^1.0.3",
    "tween24": "^1.4.1"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^2.2.2",
    "astro": "^1.3.0",
    "autoprefixer": "^10.4.13",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "markuplint": "^2.11.0",
    "postcss": "^8.4.19",
    "postcss-extract-media-query": "^3.0.0",
    "postcss-preset-env": "^7.8.2",
    "postcss-sort-media-queries": "^4.3.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "svg-sprite": "^2.0.2"
  },
  "browserslist": [
    "last 2 major versions",
    "not dead"
  ]
}
